<%= form_for([@contact, @reporting], html: {multipart: true}) do |f| %>
	
	<div class="form-group">
		<%= f.label :title, '内容' %>
		<%= f.text_field :title, class: 'form-control' %>
	</div>

	<div class="form-group">
		<%= f.label :message, 'メッセージ' %>
		<%= f.text_area :message, class: 'form-control' %>
	</div>

	<div class="form-group">
		<%= f.submit '送信する', class: "btn btn-info" %>
	</div>
<% end %>